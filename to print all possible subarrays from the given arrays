import java.util.Scanner;
public class arrayExample{
    public static void main (String[]args){
        Scanner sc=new Scanner(System.in);
        int[] a={10,20,30,40,50};
        int n= a.length;
        for( int j=0;j<n;j++){
            for(int k=0;k<=j;k++){
                System.out.print(a[k]+" ");
            }
            System.out.println();
        }
    }
}
